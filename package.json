{
  "name": "OHQ",
  "version": "1.0.0",
  "description": "Office Hours Queue for Penn",
  "main": "frontend/index.js",
  "repository": "https://github.com/pennlabs/OHQ.git",
  "homepage": "https://github.com/pennlabs/OHQ",
  "engines": {
    "node": "6.9.1"
  },
  "bugs": {
    "url": "https://github.com/pennlabs/OHQ/issues"
  },
  "scripts": {
    "clean": "rimraf ApiServer/public/js && rimraf ApiServer/public/css && rimraf ApiServer/public/images",
    "start": "NODE_ENV=development node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "build": "npm run clean && npm run postinstall",
    "postinstall": "NODE_ENV=production webpack --progress --display-reasons --display-modules",
    "prod": "node ./ApiServer/index.js",
    "server": "nodemon ./ApiServer/index.js",
    "lint": "eslint **/",
    "test": "npm run lint -- --quiet && mocha --compilers js:babel-core/register --require ignore-styles --require babel-polyfill --require ./frontend/test/test_helper.js --recursive ./frontend/test",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Penn Labs",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "eslint": "^3.1.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^2.0.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.4.1",
    "mocha": "^2.5.3",
    "nodemon": "^1.10.2",
    "react-addons-test-utils": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.4",
    "webpack-dev-server": "^1.14.1"
  },
